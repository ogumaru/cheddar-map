{
  "name": "cheddar-map",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run copy && npm run build:prod",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production --node-env=production",
    "watch": "webpack --watch",
    "serve": "webpack serve",
    "copy": "npm run copy:core && npm run copy:calcite",
    "copy:core": "mkdirp dist/core && ncp node_modules/@arcgis/core/assets/ dist/core",
    "copy:calcite": "mkdirp dist/calcite && ncp node_modules/@esri/calcite-components/dist/calcite/assets/ dist/calcite"
  },
  "devDependencies": {
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "css-loader": "^6.7.1",
    "eslint": "^8.26.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.3.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.5.0",
    "mkdirp": "^1.0.4",
    "ncp": "^2.0.0",
    "prettier": "^2.6.2",
    "ts-loader": "^9.3.0",
    "typescript": "^4.8.4",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.2"
  },
  "dependencies": {
    "@arcgis/core": "^4.23.7",
    "@esri/calcite-components-react": "^0.32.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "engines": {
    "node": ">= 16",
    "npm": ">= 8.5"
  }
}
